{
  "name": "icplan-server-nest",
  "version": "0.0.1",
  "description": "ICPlan server using Nest.js & TypeORM",
  "main": "dist/apps/api/src/main.js",
  "author": "IOMechs",
  "private": true,
  "license": "ISC",
  "scripts": {
    "build-api": "nest build api",
    "build-admin": "nest build admin",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:api": "nest start api",
    "start:admin": "nest start admin",
    "debug:api": "nest start api --debug --watch",
    "debug:admin": "nest start admin --debug --watch",
    "prod:api": "node dist/apps/api/main",
    "prod:admin": "node dist/apps/admin/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.864.0",
    "@aws-sdk/client-route-53": "^3.864.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/lib-storage": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@sentry/node": "^10.5.0",
    "@sentry/tracing": "^7.120.4",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "docx-template": "^2.2.5",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-saml": "^3.2.4",
    "pg": "^8.16.3",
    "puppeteer": "^24.16.2",
    "redis": "^5.8.1",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rrule": "^2.8.1",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.17.0",
    "stripe": "^18.4.0",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/activity_log(|/.*)$": "<rootDir>/libs/activity_log/src/$1",
      "^@app/administrator(|/.*)$": "<rootDir>/libs/administrator/src/$1",
      "^@app/audience(|/.*)$": "<rootDir>/libs/audience/src/$1",
      "^@app/budget(|/.*)$": "<rootDir>/libs/budget/src/$1",
      "^@app/business_area(|/.*)$": "<rootDir>/libs/business_area/src/$1",
      "^@app/channel(|/.*)$": "<rootDir>/libs/channel/src/$1",
      "^@app/color(|/.*)$": "<rootDir>/libs/color/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/communication(|/.*)$": "<rootDir>/libs/communication/src/$1",
      "^@app/company(|/.*)$": "<rootDir>/libs/company/src/$1",
      "^@app/content_type(|/.*)$": "<rootDir>/libs/content_type/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/file(|/.*)$": "<rootDir>/libs/file/src/$1",
      "^@app/grid_media_contact(|/.*)$": "<rootDir>/libs/grid_media_contact/src/$1",
      "^@app/location(|/.*)$": "<rootDir>/libs/location/src/$1",
      "^@app/notification(|/.*)$": "<rootDir>/libs/notification/src/$1",
      "^@app/package(|/.*)$": "<rootDir>/libs/package/src/$1",
      "^@app/parent_folder(|/.*)$": "<rootDir>/libs/parent_folder/src/$1",
      "^@app/phase(|/.*)$": "<rootDir>/libs/phase/src/$1",
      "^@app/plan(|/.*)$": "<rootDir>/libs/plan/src/$1",
      "^@app/risk(|/.*)$": "<rootDir>/libs/risk/src/$1",
      "^@app/scheduler(|/.*)$": "<rootDir>/libs/scheduler/src/$1",
      "^@app/strategic_priority(|/.*)$": "<rootDir>/libs/strategic_priority/src/$1",
      "^@app/style(|/.*)$": "<rootDir>/libs/style/src/$1",
      "^@app/subscription(|/.*)$": "<rootDir>/libs/subscription/src/$1",
      "^@app/task(|/.*)$": "<rootDir>/libs/task/src/$1",
      "^@app/user(|/.*)$": "<rootDir>/libs/user/src/$1"
    }
  }
}